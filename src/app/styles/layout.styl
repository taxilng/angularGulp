.l-main
    height: 100%;
    padding: 0;
.l-header
    width: 100%;
    height: 64px;
    position: fixed;
    z-index: 2;
    background-color #0c92f2

.l-body
    width: 100%;
    height: 100%;
    padding-top: 60px;
    position:fixed;
.l-menu
    position fixed
    width 208px
    top 64px
    height: 100%;
    z-index 5
    background #22282e
    color #fff
    bottom 0
.l-menu-fold
    width 70px
.l-content
    position absolute
    width: 100%;
    height: 100%;
    left 208px
    z-index 2
    padding: 0 0 0 0px;
.l-content-fold
    left 70px
.l-view
    height: 92%;
    width: 100%;
    overflow: auto;
    padding 0
.l-wizard
    margin-bottom:-5px;
.l-horizontal-center
    text-align: center;
.page-sidebar
    padding 0
    .sidebar-inner
        width 100%
        height 100%
        overflow auto
        overflow-x hidden
        .sidebar-fold
            height 30px
            width 100%
            background #394555
            color #aeb9c2
            line-height 30px !important
            font-size 12px
            cursor pointer
            -webkit-user-select none
.l-menu-mini
    width 50px
    .icon-title
        display:none
        
// .content-wrapper
    
.main-footer
    text-align center
    margin 0

.content-wrapper, .right-side, .main-footer
    transition none !important
    strong
        color #444
        font-size 14px
        margin-left 5px
.percent-sign
    position: absolute;
    top: 7px;
    right: 15px;
    z-index: 10;
.cfo-tree
    .cfo-tree-panel
        background-color: #fff;
    .btn-success
        background-color: #fc6969;
        border-color: #fc6969;
        &:hover
            background-color: #fc6969;
            border-color: #fc6969;
    .angular-ui-tree-handle
        background: #f8faff;
        color: #7c9eb2;
        &:hover
            color: #438eb9;
            // background: #f4f6f7;
            border-color: #dce2e8;
    .angular-ui-tree-placeholder
        background: #f0f9ff;
    .tree-node
        background #fff;
        color: #333;
        font-size 14px
        &:hover
            // background $color-gray-primary
    .tree-handle
        background: #428bca;
        color: #fff;
    .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-handle .btn-success
        background-color: #21bcee;
        border-color: #21bcee;
    .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-handle .btn-success
        background-color: #ff9400;
        border-color: #ff9400;
    .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-nodes .angular-ui-tree-node .angular-ui-tree-handle .btn-success
        background-color: #ef8698;
        border-color: #ef8698;
    .selected
        &>.tree-node
            background: $color-gray-primary;
            color: $color-blue-primary;
            .btn-success
                background: $color-white;
                color: $color-red-primary;

